<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.TextFlow?>

<ScrollPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="demo.DemoAuthenticationController">
   <content>
      <AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" stylesheets="@../css/demo_content.css">
         <children>
            <TextArea fx:id="demoATextArea" layoutX="260.0" layoutY="33.0" prefHeight="350.0" prefWidth="300.0" />
            <Button fx:id="nextButton" layoutX="350.0" layoutY="536.0" mnemonicParsing="false" onAction="#next" prefHeight="40.0" prefWidth="120.0" text="&gt;&gt;">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Label fx:id="userLabel" layoutX="50.0" layoutY="33.0" prefHeight="40.0" prefWidth="129.0" text="Sender">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextFlow fx:id="textFlow" layoutX="30.0" layoutY="399.0" prefHeight="120.0" prefWidth="740.0" />
            <Button fx:id="schemeButton" layoutX="641.0" layoutY="33.0" mnemonicParsing="false" onAction="#showScheme" prefHeight="40.0" prefWidth="129.0" text="Show Scheme" />
            <Button fx:id="backButton" layoutX="30.0" layoutY="536.0" mnemonicParsing="false" onAction="#returnToMenu" prefHeight="40.0" prefWidth="130.0" text="Return to Menu">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
