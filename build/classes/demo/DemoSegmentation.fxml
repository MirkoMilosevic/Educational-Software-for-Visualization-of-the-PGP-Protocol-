<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.TextFlow?>

<ScrollPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="demo.DemoSegmentationController">
   <content>
      <AnchorPane id="AnchorPane" prefHeight="650.0" prefWidth="800.0" stylesheets="@../css/demo_content.css">
         <children>
            <TextArea fx:id="segmentationTextArea" layoutX="250.0" layoutY="25.0" prefHeight="350.0" prefWidth="300.0" />
            <Button fx:id="nextButton" layoutX="340.0" layoutY="585.0" mnemonicParsing="false" onAction="#next" prefHeight="40.0" prefWidth="120.0" text="&gt;&gt;">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <TextFlow fx:id="segmentationTextFlow" layoutX="30.0" layoutY="443.0" prefHeight="120.0" prefWidth="740.0" />
            <Label fx:id="segmentationLabel" layoutX="250.0" layoutY="386.0" prefHeight="33.0" prefWidth="300.0" />
            <Button fx:id="backButton" layoutX="30.0" layoutY="585.0" mnemonicParsing="false" onAction="#returnToMenu" prefHeight="40.0" prefWidth="130.0" text="Return to Menu">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
